{
  "description": "Per-endpoint request rate, error rate, and latency with drill-down",
  "editable": true,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "collapsed": false,
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 0},
      "id": 100,
      "title": "Stats for selected endpoint(s)",
      "type": "row"
    },
    {
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {"mode": "absolute", "steps": [{"color": "blue", "value": null}]},
          "unit": "reqps",
          "decimals": 2
        },
        "overrides": []
      },
      "gridPos": {"h": 4, "w": 6, "x": 0, "y": 1},
      "id": 1,
      "options": {"colorMode": "value", "graphMode": "area", "justifyMode": "center", "reduceOptions": {"calcs": ["lastNotNull"]}},
      "pluginVersion": "11.4.0",
      "targets": [{
        "datasource": {"type": "prometheus", "uid": "prometheus"},
        "expr": "sum(rate(otel_http_server_request_duration_seconds_count{exported_job=\"otel-demo\", http_route=~\"$endpoint\"}[$__rate_interval]))",
        "instant": true,
        "legendFormat": "req/s",
        "refId": "A"
      }],
      "title": "Request Rate",
      "type": "stat"
    },
    {
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {"mode": "absolute", "steps": [
            {"color": "green", "value": null},
            {"color": "yellow", "value": 1},
            {"color": "red", "value": 5}
          ]},
          "unit": "percent",
          "decimals": 1,
          "min": 0,
          "max": 100
        },
        "overrides": []
      },
      "gridPos": {"h": 4, "w": 6, "x": 6, "y": 1},
      "id": 2,
      "options": {"colorMode": "background", "graphMode": "area", "justifyMode": "center", "reduceOptions": {"calcs": ["lastNotNull"]}},
      "pluginVersion": "11.4.0",
      "targets": [{
        "datasource": {"type": "prometheus", "uid": "prometheus"},
        "expr": "100 * sum(rate(otel_http_server_request_duration_seconds_count{exported_job=\"otel-demo\", http_route=~\"$endpoint\", http_response_status_code=~\"[45]..\"}[$__rate_interval])) / sum(rate(otel_http_server_request_duration_seconds_count{exported_job=\"otel-demo\", http_route=~\"$endpoint\"}[$__rate_interval]))",
        "instant": true,
        "legendFormat": "error %",
        "refId": "A"
      }],
      "title": "Error Rate",
      "type": "stat"
    },
    {
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {"mode": "absolute", "steps": [
            {"color": "green", "value": null},
            {"color": "yellow", "value": 200},
            {"color": "red", "value": 1000}
          ]},
          "unit": "ms",
          "decimals": 0
        },
        "overrides": []
      },
      "gridPos": {"h": 4, "w": 6, "x": 12, "y": 1},
      "id": 3,
      "options": {"colorMode": "value", "graphMode": "none", "justifyMode": "center", "reduceOptions": {"calcs": ["lastNotNull"]}},
      "pluginVersion": "11.4.0",
      "targets": [{
        "datasource": {"type": "prometheus", "uid": "prometheus"},
        "expr": "histogram_quantile(0.50, sum(rate(otel_http_server_request_duration_seconds_bucket{exported_job=\"otel-demo\", http_route=~\"$endpoint\"}[$__rate_interval])) by (le)) * 1000",
        "instant": true,
        "legendFormat": "p50",
        "refId": "A"
      }],
      "title": "p50 Latency",
      "type": "stat"
    },
    {
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {"mode": "absolute", "steps": [
            {"color": "green", "value": null},
            {"color": "yellow", "value": 500},
            {"color": "red", "value": 2000}
          ]},
          "unit": "ms",
          "decimals": 0
        },
        "overrides": []
      },
      "gridPos": {"h": 4, "w": 6, "x": 18, "y": 1},
      "id": 4,
      "options": {"colorMode": "value", "graphMode": "none", "justifyMode": "center", "reduceOptions": {"calcs": ["lastNotNull"]}},
      "pluginVersion": "11.4.0",
      "targets": [{
        "datasource": {"type": "prometheus", "uid": "prometheus"},
        "expr": "histogram_quantile(0.99, sum(rate(otel_http_server_request_duration_seconds_bucket{exported_job=\"otel-demo\", http_route=~\"$endpoint\"}[$__rate_interval])) by (le)) * 1000",
        "instant": true,
        "legendFormat": "p99",
        "refId": "A"
      }],
      "title": "p99 Latency",
      "type": "stat"
    },

    {
      "collapsed": false,
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 5},
      "id": 101,
      "title": "Traffic & Errors",
      "type": "row"
    },
    {
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "fieldConfig": {
        "defaults": {"unit": "reqps"},
        "overrides": []
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 6},
      "id": 5,
      "options": {
        "legend": {"calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom"},
        "tooltip": {"mode": "multi"}
      },
      "targets": [{
        "datasource": {"type": "prometheus", "uid": "prometheus"},
        "expr": "sum(rate(otel_http_server_request_duration_seconds_count{exported_job=\"otel-demo\", http_route=~\"$endpoint\"}[$__rate_interval])) by (http_request_method, http_route)",
        "legendFormat": "{{http_request_method}} {{http_route}}",
        "refId": "A"
      }],
      "title": "Request Rate",
      "type": "timeseries"
    },
    {
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "fieldConfig": {
        "defaults": {"unit": "reqps"},
        "overrides": [
          {"matcher": {"id": "byName", "options": "2xx"}, "properties": [{"id": "color", "value": {"fixedColor": "green", "mode": "fixed"}}]},
          {"matcher": {"id": "byName", "options": "4xx"}, "properties": [{"id": "color", "value": {"fixedColor": "yellow", "mode": "fixed"}}]},
          {"matcher": {"id": "byName", "options": "5xx"}, "properties": [{"id": "color", "value": {"fixedColor": "red", "mode": "fixed"}}]}
        ]
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 6},
      "id": 6,
      "options": {
        "legend": {"calcs": ["mean"], "displayMode": "table", "placement": "bottom"},
        "tooltip": {"mode": "multi"}
      },
      "targets": [
        {
          "datasource": {"type": "prometheus", "uid": "prometheus"},
          "expr": "sum(rate(otel_http_server_request_duration_seconds_count{exported_job=\"otel-demo\", http_route=~\"$endpoint\", http_response_status_code=~\"2..\"}[$__rate_interval]))",
          "legendFormat": "2xx",
          "refId": "A"
        },
        {
          "datasource": {"type": "prometheus", "uid": "prometheus"},
          "expr": "sum(rate(otel_http_server_request_duration_seconds_count{exported_job=\"otel-demo\", http_route=~\"$endpoint\", http_response_status_code=~\"4..\"}[$__rate_interval]))",
          "legendFormat": "4xx",
          "refId": "B"
        },
        {
          "datasource": {"type": "prometheus", "uid": "prometheus"},
          "expr": "sum(rate(otel_http_server_request_duration_seconds_count{exported_job=\"otel-demo\", http_route=~\"$endpoint\", http_response_status_code=~\"5..\"}[$__rate_interval]))",
          "legendFormat": "5xx",
          "refId": "C"
        }
      ],
      "title": "Response Status Codes",
      "type": "timeseries"
    },

    {
      "collapsed": false,
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 14},
      "id": 102,
      "title": "Latency",
      "type": "row"
    },
    {
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "fieldConfig": {
        "defaults": {"unit": "ms"},
        "overrides": []
      },
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 15},
      "id": 7,
      "options": {
        "legend": {"calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom"},
        "tooltip": {"mode": "multi"}
      },
      "targets": [
        {
          "datasource": {"type": "prometheus", "uid": "prometheus"},
          "expr": "histogram_quantile(0.50, sum(rate(otel_http_server_request_duration_seconds_bucket{exported_job=\"otel-demo\", http_route=~\"$endpoint\"}[$__rate_interval])) by (le, http_route)) * 1000",
          "legendFormat": "p50 {{http_route}}",
          "refId": "A"
        },
        {
          "datasource": {"type": "prometheus", "uid": "prometheus"},
          "expr": "histogram_quantile(0.95, sum(rate(otel_http_server_request_duration_seconds_bucket{exported_job=\"otel-demo\", http_route=~\"$endpoint\"}[$__rate_interval])) by (le, http_route)) * 1000",
          "legendFormat": "p95 {{http_route}}",
          "refId": "B"
        },
        {
          "datasource": {"type": "prometheus", "uid": "prometheus"},
          "expr": "histogram_quantile(0.99, sum(rate(otel_http_server_request_duration_seconds_bucket{exported_job=\"otel-demo\", http_route=~\"$endpoint\"}[$__rate_interval])) by (le, http_route)) * 1000",
          "legendFormat": "p99 {{http_route}}",
          "refId": "C"
        }
      ],
      "title": "Latency Percentiles by Endpoint",
      "type": "timeseries"
    },

    {
      "collapsed": false,
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 23},
      "id": 103,
      "title": "All Endpoints Summary",
      "type": "row"
    },
    {
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "fieldConfig": {
        "defaults": {"unit": "reqps"},
        "overrides": [
          {
            "matcher": {"id": "byName", "options": "p50 (ms)"},
            "properties": [{"id": "unit", "value": "ms"}, {"id": "decimals", "value": 0}]
          },
          {
            "matcher": {"id": "byName", "options": "p99 (ms)"},
            "properties": [{"id": "unit", "value": "ms"}, {"id": "decimals", "value": 0},
              {"id": "thresholds", "value": {"mode": "absolute", "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 200},
                {"color": "red", "value": 1000}
              ]}},
              {"id": "custom.displayMode", "value": "color-background"}
            ]
          },
          {
            "matcher": {"id": "byName", "options": "errors/s"},
            "properties": [{"id": "unit", "value": "reqps"},
              {"id": "thresholds", "value": {"mode": "absolute", "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 0.001}
              ]}},
              {"id": "custom.displayMode", "value": "color-background"}
            ]
          }
        ]
      },
      "gridPos": {"h": 10, "w": 24, "x": 0, "y": 24},
      "id": 8,
      "options": {
        "footer": {"show": false},
        "showHeader": true,
        "sortBy": [{"displayName": "req/s", "desc": true}]
      },
      "targets": [
        {
          "datasource": {"type": "prometheus", "uid": "prometheus"},
          "expr": "sum(rate(otel_http_server_request_duration_seconds_count{exported_job=\"otel-demo\"}[$__rate_interval])) by (http_request_method, http_route)",
          "instant": true,
          "legendFormat": "{{http_request_method}} {{http_route}}",
          "refId": "A",
          "format": "table"
        },
        {
          "datasource": {"type": "prometheus", "uid": "prometheus"},
          "expr": "sum(rate(otel_http_server_request_duration_seconds_count{exported_job=\"otel-demo\", http_response_status_code=~\"[45]..\"}[$__rate_interval])) by (http_request_method, http_route)",
          "instant": true,
          "legendFormat": "{{http_request_method}} {{http_route}}",
          "refId": "B",
          "format": "table"
        },
        {
          "datasource": {"type": "prometheus", "uid": "prometheus"},
          "expr": "histogram_quantile(0.50, sum(rate(otel_http_server_request_duration_seconds_bucket{exported_job=\"otel-demo\"}[$__rate_interval])) by (le, http_request_method, http_route)) * 1000",
          "instant": true,
          "legendFormat": "{{http_request_method}} {{http_route}}",
          "refId": "C",
          "format": "table"
        },
        {
          "datasource": {"type": "prometheus", "uid": "prometheus"},
          "expr": "histogram_quantile(0.99, sum(rate(otel_http_server_request_duration_seconds_bucket{exported_job=\"otel-demo\"}[$__rate_interval])) by (le, http_request_method, http_route)) * 1000",
          "instant": true,
          "legendFormat": "{{http_request_method}} {{http_route}}",
          "refId": "D",
          "format": "table"
        }
      ],
      "transformations": [
        {"id": "merge", "options": {}},
        {
          "id": "organize",
          "options": {
            "renameByName": {
              "Value #A": "req/s",
              "Value #B": "errors/s",
              "Value #C": "p50 (ms)",
              "Value #D": "p99 (ms)",
              "http_request_method": "Method",
              "http_route": "Route",
              "Time": ""
            },
            "excludeByName": {"Time": true, "instance": true, "job": true, "exported_instance": true, "exported_job": true}
          }
        }
      ],
      "title": "All Endpoints — Rate / Errors / Latency",
      "type": "table"
    }
  ],
  "refresh": "10s",
  "schemaVersion": 38,
  "tags": ["otel-demo", "spring-boot", "endpoints"],
  "templating": {
    "list": [
