        "query": {
          "query": "label_values(otel_http_server_request_duration_seconds_count{exported_job=\"otel-demo\"}, http_route)",
          "refId": "StandardVariableQuery"
        },
        "refresh": 2,
        "regex": "",
        "sort": 1,
        "type": "query"
      }
    ]
  },
  "time": {"from": "now-30m", "to": "now"},
