        "datasource": {"type": "prometheus", "uid": "prometheus"},
        "expr": "100 * sum(rate(otel_http_server_request_duration_seconds_count{exported_job=\"otel-demo\", http_response_status_code=~\"[45]..\"}[$__rate_interval])) / sum(rate(otel_http_server_request_duration_seconds_count{exported_job=\"otel-demo\"}[$__rate_interval]))",
