      "pluginVersion": "11.4.0",
      "targets": [{
        "datasource": {"type": "prometheus", "uid": "prometheus"},
        "expr": "sum(rate(otel_http_server_request_duration_seconds_count{exported_job=\"otel-demo\"}[$__rate_interval]))",
